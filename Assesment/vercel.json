{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/quiz/(.*)",
      "dest": "/quiz/$1"
    },
    {
      "src": "/instructions",
      "dest": "/quiz/instructions.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
