{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "Assesment/api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/quiz/(.*)",
      "dest": "/Assesment/api/quiz/$1.js"
    },
    {
      "src": "/quiz/instructions",
      "dest": "/Assesment/quiz/instructions.html"
    },
    {
      "src": "/assessment",
      "dest": "/Assesment/quiz/standalone.html"
    },
    {
      "src": "/quiz",
      "dest": "/Assesment/quiz/standalone.html"
    },
    {
      "src": "/Assesment/quiz/standalone-engine.js",
      "dest": "/Assesment/quiz/standalone-engine.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/img/(.*)",
      "dest": "/img/$1"
    },
    {
      "src": "/logos/(.*)",
      "dest": "/logos/$1"
    },
    {
      "src": "/pdfs/(.*)",
      "dest": "/pdfs/$1"
    },
    {
      "src": "/Assesment/quiz/pdfs/(.*)",
      "dest": "/Assesment/quiz/pdfs/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
