{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "functions": {
    "Assesment/api/**/*.js": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/quiz/:path*",
      "destination": "/Assesment/api/quiz/:path*"
    },
    {
      "source": "/quiz/instructions",
      "destination": "/Assesment/quiz/instructions.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
